@model Contrato
@{
    ViewData["Title"] = "Contrato";
}

<div class="col-md-12">
    <div class="form-group">
        <label>Observação</label>
        <textarea asp-for="Conteudo" disabled rows="10" cols="80">
                    </textarea>
    </div>
</div>

@section Scripts{
    <script src="~/ckeditor-full/ckeditor.js"></script>
    <script>
        // Replace the <textarea id="editor1"> with a CKEditor
        // instance, using default configuration.
        CKEDITOR.replace('Conteudo').config.height = '500px';
    </script>
    <script src="~/js/Aula.js"></script>
}

